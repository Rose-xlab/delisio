{
  "name": "delisio",
  "description": "Backend for Cooking-Assistant app with AI-generated recipes",
  "version": "1.0.0",
  "private": true,
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "dev:server": "set PORT=3002 && nodemon --watch src --ext ts,json --exec ts-node src/server.ts",
    "dev:chat-worker": "nodemon --watch src --ext ts,json --exec ts-node src/workers/chat.worker.ts",
    "dev:image-worker": "nodemon --watch src --ext ts,json --exec ts-node src/workers/image.worker.ts",
    "dev:recipe-worker": "nodemon --watch src --ext ts,json --exec ts-node src/workers/recipe.worker.ts",
    "dev": "concurrently \"npm:dev:server\" \"npm:dev:chat-worker\" \"npm:dev:image-worker\" \"npm:dev:recipe-worker\"",
    "start:server": "node dist/server.js",
    "start:chat-worker": "node dist/workers/chat.worker.js",
    "start:image-worker": "node dist/workers/image.worker.js",
    "start:recipe-worker": "node dist/workers/recipe.worker.js",
    "start": "concurrently \"npm:start:server\" \"npm:start:chat-worker\" \"npm:start:image-worker\" \"npm:start:recipe-worker\"",
    "postinstall": "npm run build",
    "gen:types": "npx supabase gen types typescript --project-id kwdnztyhexydoaicjepd --schema public,auth > src/types/supabase.ts"
  },
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@bull-board/api": "^5.23.0",
    "@bull-board/express": "^5.23.0",
    "@sentry/node": "^7.120.3",
    "@sentry/profiling-node": "^9.14.0",
    "@sentry/tracing": "^7.120.3",
    "@supabase/supabase-js": "^2.49.4",
    "@types/uuid": "^10.0.0",
    "axios": "^1.8.4",
    "bullmq": "^5.49.1",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "joi": "^17.12.2",
    "node-fetch": "^2.6.9",
    "openai": "^4.95.0",
    "rate-limiter-flexible": "^7.0.0",
    "stripe": "^15.4.0",
    "uuid": "^11.1.0",
    "winston": "^3.12.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.17.30",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.9",
    "supabase": "^1.100.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
